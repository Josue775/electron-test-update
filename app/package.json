{
  "name": "electron-app2",
  "version": "1.0.5",
  "description": "My test electron app2",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon --exec electron .",
    "start": "electron .",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/Josue775/electron-test-update",
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "build": {
    "appId": "com.josue.Autoupdater2",
    "productName": "Autoupdater app2",
    "win": {
      "target": "nsis"
    },
    "directories": {
      "output": "dist"
    }
  },
  "author": "Josue",
  "license": "ISC",
  "devDependencies": {
    "electron": "^25.1.1",
    "electron-builder": "^24.4.0",
    "nodemon": "^2.0.19"
  },
  "dependencies": {
    "electron-updater": "^6.1.1",
    "electron-notifications": "^1.0.0"
  }
}
